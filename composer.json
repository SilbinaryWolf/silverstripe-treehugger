{
    "name": "silbinarywolf/silverstripe-treehugger",
    "description": "Add additional menus (such as footer, sidebars) programmatically that can be managed by CMS users nicely on the SiteConfig or Multisite view.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "sortablemenu",
        "menu",
        "siteconfig",
        "multisites",
        "symbiote"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jake Bentvelzen",
            "email": "jake@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/cms": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "phpcbf": "phpcbf code/ tests/",
        "phpcs": "phpcs code/ tests/"
    },
    "suggest": {
        "silverstripe/silverstripe-siteconfig": "Compatible with SiteConfig module.",
        "symbiote/silverstripe-multisites": "Compatible with Multisites module.",
        "symbiote/silverstripe-gridfieldextensions": "Install to sort menu order via SiteConfig or Multisites"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\SortableMenu\\": "code/",
            "Symbiote\\SortableMenu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
